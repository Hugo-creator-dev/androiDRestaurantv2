<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="fr.isen.musoles.androidrestaurantv2" >

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/resto"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar"
        android:usesCleartextTraffic="true" >
        <activity
            android:name=".activity.specialPage.ErrorActivity"
            android:exported="false" />
        <activity
            android:name=".activity.order.PastOrderActivity"
            android:exported="false" />
        <activity
            android:name=".activity.order.OrderWaitActivity"
            android:exported="false" />
        <activity
            android:name=".activity.user.LoginActivity"
            android:exported="false" />
        <activity
            android:name=".activity.order.ShopActivity"
            android:exported="false" />
        <activity
            android:name=".activity.basicNavigation.DetailActivity"
            android:exported="false" />
        <activity
            android:name=".activity.basicNavigation.CategorieActivity"
            android:exported="false" />
        <activity
            android:name=".activity.basicNavigation.MenuActivity"
            android:exported="false" />
        <activity
            android:name=".activity.specialPage.HomeActivity"
            android:exported="true" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>